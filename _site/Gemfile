source "https://rubygems.org"

gem "github-pages", group: :jekyll_plugins

# comentado porque gera erro no windows (https://github.com/jekyll/jekyll/issues/3721)    gem 'wdm', '>= 0.1.0' if Gem.win_platform?

group :jekyll_plugins do
  gem "jekyll-redirect-from"
  gem "jekyll-paginate"
  gem "jekyll-sitemap"
  gem "jekyll-gist"
  gem "jekyll-feed"
  gem "jemoji"
end